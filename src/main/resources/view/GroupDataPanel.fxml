<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<SplitPane dividerPositions="0.5" minWidth="680.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="5" left="10" right="10" top="5" />
   </padding>
   <items>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <ListView fx:id="studentListView" VBox.vgrow="ALWAYS" />
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <ListView fx:id="lessonListView" VBox.vgrow="ALWAYS" />
      </VBox>
   </items>
</SplitPane>
